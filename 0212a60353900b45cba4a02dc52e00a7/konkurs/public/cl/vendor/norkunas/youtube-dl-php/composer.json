{
    "name": "norkunas/youtube-dl-php",
    "type": "library",
    "description": "youtube-dl wrapper for php",
    "keywords": ["youtube", "youtube-dl"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tomas NorkÅ«nas",
            "email": "norkunas.tom@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "ext-json": "*",
        "symfony/filesystem": "^4.4|^5.1",
        "symfony/polyfill-php80": "^1.18",
        "symfony/process": "^4.4|^5.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "mikey179/vfsstream": "^1.6.7",
        "phpstan/phpstan": "^0.12.42",
        "phpstan/phpstan-phpunit": "^0.12.16",
        "symfony/phpunit-bridge": "^5.1.5"
    },
    "autoload": {
        "psr-4": { "YoutubeDl\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "YoutubeDl\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
